{
    "name": "technoob-lambda-functions",
    "description": "Technoob Serverless Functions AWS",
    "version": "0.0.1",
    "private": true,
    "type": "module",
    "devDependencies": {
        "jest": "^29.2.1",
        "puppeteer": "^22.4.1"
    },
    "scripts": {
        "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test)/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[jt]s?(x)",
            "**/__tests__/**/*.mjs?(x)",
            "**/?(*.)+(spec|test).mjs?(x)"
        ],
        "moduleFileExtensions": [
            "mjs",
            "js"
        ]
    },
    "dependencies": {
        "@honeybadger-io/js": "^6.8.3",
        "@sparticuz/chromium": "^119.0.0",
        "axios": "^1.6.7",
        "dotenv": "^16.4.5",
        "puppeteer-core": "^21.5.0",
        "puppeteer-extra": "^3.3.6",
        "puppeteer-extra-plugin-stealth": "^2.11.2"
    }
}
